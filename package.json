{
  "name": "autoscrape-www",
  "version": "0.1.0",
  "description": "Frontend web UI for running AutoScrape and extracting data.",
  "main": "src/index.js",
  "scripts": {
    "download-hext": "rm -f public/hext-emscripten.js public/hext-emscripten.wasm && wget https://github.com/html-extract/hext-emscripten/releases/download/v0.1.4/hext-emscripten.wasm -P public/ && wget https://github.com/html-extract/hext-emscripten/releases/download/v0.1.4/hext-emscripten.js -P public/",
    "start": "npm run download-hext && react-scripts start",
    "build": "npm run download-hext && react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/brandonrobertz/autoscrape-www.git"
  },
  "author": "Brandon Roberts",
  "license": "AGPL-3.0",
  "bugs": {
    "url": "https://github.com/brandonrobertz/autoscrape-www/issues"
  },
  "homepage": "https://github.com/brandonrobertz/autoscrape-www#readme",
  "devDependencies": {
    "css-loader": "^2.1.0",
    "html-webpack-inline-source-plugin": "^1.0.0-beta.2",
    "html-webpack-plugin": "^4.0.0-beta.5",
    "http-server": "^0.11.1",
    "style-loader": "^0.23.1",
    "webpack": "^4.28.3",
    "webpack-cli": "^3.2.0"
  },
  "dependencies": {
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.4.0",
    "@testing-library/user-event": "^7.2.1",
    "datatransfer-files-promise": "^1.2.0",
    "file-saver": "^2.0.0",
    "jquery": "^3.4.1",
    "jszip": "^3.1.5",
    "object-assign-deep": "^0.4.0",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-redux": "^7.1.3",
    "react-scripts": "3.3.1",
    "redux": "^4.0.5"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
